class Solution {
    public int maxProfit(int[] prices) {
        
        int profit = 0;
        int minValue = prices[0];
        
        for (int i = 1; i < prices.length; i++) {
            if (prices[i] < minValue) minValue = prices[i];
            
            if (profit < prices[i] - minValue) profit = prices[i] - minValue;
        }
        
        return profit;
    }
}